<Page x:Class="SupportApp.Forside"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:SupportApp"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Forside">

    <Grid>
        <ListBox Padding="70">
            <Button x:Name="btnArticles" Click="btnArticles_Click" Background="Green" Foreground="White" Height="70" Width="160" FontSize="20" FontWeight="DemiBold" Cursor="Hand">Artikler</Button>
            <Button x:Name="btnVideos" Click="btnVideos_Click" Background="Green" Foreground="White" Height="70" Width="160" FontSize="20" FontWeight="DemiBold" Cursor="Hand">Videoer</Button>
            <Button x:Name="btnManual" Click="BtnManual_Click" Background="Green" Foreground="White" Height="70" Width="160" FontSize="20" FontWeight="DemiBold" Cursor="Hand">Manual</Button>
            <Button x:Name="btnCalipers" Click="BtnCalipers_Click" Background="Green" Foreground="White" Height="70" Width="160" FontSize="20" FontWeight="DemiBold" Cursor="Hand">Kalibre</Button>
        </ListBox>
        
    </Grid>
</Page>
